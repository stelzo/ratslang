{
  "name": "ratslang",
  "displayName": "Ratslang",
  "description": "Syntax Highlighting for the Ratslang configuration language",
  "version": "0.3.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "publisher": "stelzo",
  "repository": {
    "type": "git",
    "url": "https://codeberg.org/uos-ce/ratslang.git"
  },
  "license": "(MIT OR Apache-2.0)",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "rl",
      "aliases": ["Ratslang", "rl"],
      "extensions": [".rl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "rl",
      "scopeName": "source.rl",
      "path": "./syntaxes/rl.tmLanguage.json"
    }]
  }
}
